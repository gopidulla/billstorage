<%= form_for(bill) do |f| %>
  <% if bill.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(bill.errors.count, "error") %> prohibited this bill from being saved:</h2>

      <ul>
      <% bill.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

   <div class="row">
  
 <div class="col-lg-6 col-md-6">
  <div class="control-group">
    <%= f.label :section, class: 'control-label' %>
    <div class="controls">

    <%= f.collection_select(:section, Section.all, :name, :name, 
    { prompt: 'Select a section' }, { id: 'section-select' }) %>

       
  </div>
  </div>
  </div>

<div class="col-lg-3 col-md-3">
  <div class="control-group">
  
    <%= f.label :year, class: 'control-label' %>
    <div class="controls">
     <%= f.select(:year, options_for_select([["2010","2010"],["2011","2011"],["2012","2012"],["2013","2013"],["2014","2014"],["2015","2015"],["2016","2016"],["2017","2017"],["2018","2018"],["2019","2019"],["2020","2020"],["2021","2021"],["2022","2022"],["2023","2023"],["2024","2024"],["2025","2025"]], :selected => f.object.year), :include_blank => true) %>
  </div>
</div>
</div>

<div class="col-lg-3 col-md-3">
  <div class="control-group">
  
    <%= f.label :month, class: 'control-label' %>
    <div class="controls">


     <%= f.select(:month, options_for_select([["JAN", "JAN"],["FEB", "FEB"],["MAR", "MAR"],["APR", "APR"],["MAY", "MAY"],["JUN", "JUN"],["JUL", "JUL"],["AUG", "AUG"],["SEP", "SEP"],["OCT", "OCT"],["NOV", "NOV"],["DEC", "DEC"]],  :selected => f.object.month), :include_blank => true) %>

  </div>
</div>
</div>
</div>
<br>


<div class="row">
 <div class="col-lg-6 col-md-6">
  <div class="control-group">
    <%= f.label :dvno, class: 'control-label' %>
    <div class="controls">
    <%= f.text_field :dvno, class: 'control-label' %>
  </div>
</div>
</div>

  <div class="col-lg-6 col-md-6">
  <div class="control-group">
    <%= f.label :Billtype, class: 'control-label' %>
    <div class="controls">

 <%= f.grouped_collection_select :typebill, Section.all, :typebills, 
    :name, :name, :name, { include_blank: 'Select a typebill' }, { id: 'typebill-select' } %>
  
   


   
  </div>
</div>
</div>
</div>
<br>

<div class="row">
  <div class="col-lg-6 col-md-6">
