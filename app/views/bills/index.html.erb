<p id="notice"><%= notice %></p>

<h1>Bills</h1>

<table>
  <thead>
    <tr>
      <th>Section</th>
      <th>Dvno</th>
      <th>Month</th>
      <th>Typebill</th>
      <th>Compactor</th>
      <th>Rack</th>
      <th>Shelf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bills.each do |bill| %>
      <tr>
        <td><%= bill.section %></td>
        <td><%= bill.dvno %></td>
        <td><%= bill.month %></td>
        <td><%= bill.typebill %></td>
        <td><%= bill.compactor %></td>
        <td><%= bill.rack %></td>
        <td><%= bill.shelf %></td>
        <td><%= link_to 'Show', bill %></td>
        <td><%= link_to 'Edit', edit_bill_path(bill) %></td>
        <td><%= link_to 'Destroy', bill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bill', new_bill_path %>
